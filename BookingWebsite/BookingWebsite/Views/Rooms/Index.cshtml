@using BookingWebsite.Models.Entities;
@addTagHelper*, Microsoft.AspNetCore.Mvc.TagHelpers
@model Room[]
<!DOCTYPE html>
<html>
    <head>
        <title>Room list</title>
    </head>
    <body>
        <h1>List of rooms</h1>
        @foreach (var item in Model)
        {
            <ul>
                <li><a asp-controller="Rooms" asp-action="Detail" asp-route-id="@item.RoomId">@item.Name</a></li>
            </ul>
        }
        <p><h4><a href="~/Home/Index">Back to main page</a></h4></p>
    </body>
</html>